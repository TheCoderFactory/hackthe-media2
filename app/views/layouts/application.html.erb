    <script type="text/javascript" src="<%= asset_path 'waypoints.min.js' %>"></script>
    <script type="text/javascript" src="<%= asset_path 'waypoints-sticky.min.js' %>"></script>
    <script type="text/javascript" src="<%= asset_path 'jquery.fancybox.js' %>"></script>
    <script type="text/javascript" src="<%= asset_path 'scripts.js' %>"></script>
</body>
</html>
